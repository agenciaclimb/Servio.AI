{
  "name": "servio-ai-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:ci": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings=1000",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "typecheck": "npx tsc -p tsconfig.build.json --noEmit",
    "test": "vitest run",
    "test:nocov": "vitest run",
    "test:file": "vitest run",
    "test:watch": "vitest watch",
    "test:fast": "vitest run --config vitest.fast.config.ts --coverage --threads false tests/AdminDashboard*.test.tsx tests/ClientDashboard.scheduleAndChat.test.tsx tests/ProspectorDashboardUnified.test.tsx tests/ProspectorDashboard.branches.test.tsx",
    "test:ui": "vitest --ui",
    "test:backend": "cd backend && node ./node_modules/vitest/vitest.mjs run --no-coverage",
    "test:all": "npm test && npm run test:backend",
    "preview": "vite preview",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:smoke": "playwright test tests/e2e/smoke/basic-smoke.spec.ts",
    "e2e:smoke:headed": "playwright test tests/e2e/smoke/basic-smoke.spec.ts --headed",
    "e2e:smoke:stripe": "playwright test tests/e2e/smoke/stripe-connect-onboarding.smoke.spec.ts",
    "e2e:critical": "playwright test tests/e2e/smoke/critical-flows.spec.ts",
    "e2e:protocol": "playwright test tests/e2e/protocol.spec.ts",
    "start:ai": "node server.js",
    "backend:start": "npm --prefix backend start",
    "backend:dev": "npm --prefix backend run dev",
    "prepare": "husky install",
    "validate": "npm run format:check && npm run lint && npm run typecheck && npm run test:all",
    "validate:prod": "npm run lint && npm run typecheck && npm test -- --run && npm run build && npm run guardrails:audit",
    "validate:doc-mestre": "tsx master_document/parser.ts DOCUMENTO_MESTRE_SERVIO_AI.md",
    "generate-tasks": "node ai-engine/gemini/generateTasks.cjs --backlog ai-tasks/TAREFAS_ATIVAS.json",
    "orchestrate-tasks": "node ai-orchestrator/src/orchestrator.cjs --tasks ai-tasks/TAREFAS_ATIVAS.json",
    "servio:full-cycle": "npm run generate-tasks && npm run orchestrate-tasks && npm test",
    "supremo:init": "node scripts/protocolo-supremo.cjs init",
    "supremo:audit": "node scripts/protocolo-supremo.cjs audit",
    "supremo:fix": "node scripts/protocolo-supremo.cjs fix",
    "supremo:test-backend": "node scripts/protocolo-supremo.cjs test-backend",
    "supremo:dashboard": "node scripts/protocolo-supremo.cjs dashboard",
    "supremo:pr-status": "node scripts/protocolo-supremo.cjs pr-status",
    "supremo:help": "node scripts/protocolo-supremo.cjs help",
    "predeploy": "npm run validate:prod",
    "precommit": "lint-staged",
    "firebase:login": "firebase login",
    "firebase:use": "firebase use",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "e2e:debug": "playwright test --debug",
    "cy:run": "npm run build && start-server-and-test preview http://localhost:4173 \"cypress run --browser chrome --headless --config-file cypress.config.cjs\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -i",
    "test:stripe": "node scripts/test_stripe.mjs",
    "validate:stripe": "powershell -ExecutionPolicy Bypass -File scripts/validate_stripe.ps1",
    "prod:smoke": "node scripts/backend_smoke_test.mjs",
    "start:seo": "node ssr-seo-server.js",
    "events:monitor": "node scripts/events-monitor.cjs",
    "guardrail:check": "node scripts/guardrails/deny-local-audit-results.cjs",
    "guardrails:audit": "node scripts/guardrails/deny-local-audit-results.cjs",
    "audit:request": "node -e \"require('fs').writeFileSync('ai-tasks/events/audit-request-PR_99.json', JSON.stringify({timestamp:new Date().toISOString(),type:'audit-request',pr_number:99},null,2))\"",
    "audit:status": "node ai-tasks/events/events-monitor.cjs status",
    "gcp:fix-firestore-iam": "powershell -ExecutionPolicy Bypass -File scripts/gcloud_fix_firestore_iam.ps1",
    "gcp:logs": "powershell -ExecutionPolicy Bypass -File scripts/gcloud_tail_logs.ps1",
    "auth:backfill-claims": "node backend/scripts/backfill-custom-claims.mjs",
    "user:backfill-uid": "node backend/scripts/backfill-user-uid.mjs",
    "functions:deploy": "firebase deploy --only functions",
    "functions:logs": "firebase functions:log",
    "audit-pr": "node automation/gemini/auditPR.js",
    "update-master": "node automation/gemini/updateMasterDoc.js",
    "apply-fix": "node automation/gemini/applyFix.js",
    "create-pr": "node automation/github/createPR.js",
    "merge-pr": "node automation/github/mergePR.js",
    "monitor:events": "npx ts-node --transpile-only ai-tasks/event-monitor.ts",
    "clean": "npm run clean:cache && npm run clean:build",
    "clean:cache": "npm cache clean --force && rimraf node_modules/.vite node_modules/.cache",
    "clean:build": "rimraf dist backend/coverage coverage .vite",
    "clean:all": "npm run clean && rimraf node_modules backend/node_modules functions/node_modules && npm install",
    "optimize": "npm run clean && npm dedupe && npm prune"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.6.0",
    "@stripe/react-stripe-js": "^2.7.1",
    "@stripe/stripe-js": "^3.5.0",
    "canvas-confetti": "^1.9.3",
    "firebase": "^12.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.17.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.1.6",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.3.1",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "rimraf": "^6.1.2",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^3.4.3",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
